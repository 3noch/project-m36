Name: project-m36
Version: 0.1
License: PublicDomain
Build-Type: Simple
Homepage: https://github.com/agentm/project-m36
Category: Relational Algebra
Cabal-Version: >= 1.2

Executable tutd
    Build-Depends: base,mtl,containers,unordered-containers,parsec,hashable,haskeline,directory,uuid
    main-is: tutd.hs
    GHC-Options: -fwarn-unused-imports -fwarn-unused-binds -fwarn-incomplete-patterns -fwarn-incomplete-uni-patterns

Test-Suite test-tutoriald
    type: exitcode-stdio-1.0
    main-is: TutorialDInterpreterTest.hs
    Build-Depends: base,HUnit,Cabal

Test-Suite test-relation
    type: exitcode-stdio-1.0
    main-is: RelationTest.hs
    Build-Depends: base,HUnit,Cabal

Test-Suite test-static-optimizer
    type: exitcode-stdio-1.0
    main-is: RelationStaticOptimizerTest.hs
    Build-Depends: base,HUnit,Cabal